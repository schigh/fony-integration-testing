							"          \"title\": \"The Value Schema\",",
							"          \"default\": \"\",",
							"          \"examples\": [",
							"            \"this is a test\"",
							"          ],",
							"          \"pattern\": \"^(.*)$\"",
							"        }",
							"      }",
							"    },",
							"    \"md5\": {",
